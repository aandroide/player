{
    "name"              : "[COLOR aqua]TG[/COLOR]_([COLOR red]Globale[/COLOR])",
    "plugin"            : "plugin.video.thegroove360",
    "priority"          : 100,
    "is_resolvable"     : "false",
    "play_movie": [
        "plugin://plugin.video.",
        {"keyboard": "{title}"},
        {"title": "(?i)^(\\[.*\\])?.*\\b{title}.*(\\({year}\\))?.*(\\[.*\\])?$"}
    ],
    "play_episode": [
        "plugin://plugin.video.",
        {"keyboard": "{showname}"},
        {"title": "(?i)^(\\[.*\\])?.*{showname}.*(TheGroove|Room|Marko86|MR_Dupont)(\\[.*\\])?$"},
        {"return": "true", "title": "(?i)^.*{season}x.?{episode}|Room.*$"},
        {"return": "true", "title": "(?i)^(\\[.*\\])?.*Stagione {season}.*(\\[.*\\])?$"},
        {"title": "(?i)^(\\[.*\\])?.*{season}x.?{episode}.*(\\[.*\\])?$"}
    ],
    "search_movie": [
        "plugin://plugin.video.",
        {"keyboard": "{title}"},
        {"dialog": "true"}
    ],
    "search_episode": [
        "plugin://plugin.video.",
        {"keyboard": "{showname}"},
        {"dialog": "true"}
    ],
    "fallback": {
        "play_movie": "TG360_player_globale.json search_movie",
        "play_episode": "TG360_player_globale.json search_episode"
    }
}
