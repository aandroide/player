    "is_resolvable"     : "false",
    "play_movie": [
        "plugin://plugin.video.",
        {"keyboard": "{title}"},
        {"title": "(?i)^(\\[.*\\])?\\b{title}.*(\\({year}\\))?(\\[.*\\])?$"},
        {"title": "(?i)^.*(supervideo|dropload|mixdrop|doodstream).*$"}
    ],
    "play_episode": [
        "plugin://plugin.video.",
        {"keyboard": "{showname}"},
        {"title": "(?i)^(\\[.*\\])?.\\b({showname}|{originaltitle}).*(\\[.*\\])?$"},
        {"title": "(?i)^.*{season}x0?{episode}.*$"}
    ],
    "search_movie": [
	    "plugin://plugin.video.",
        {"keyboard": "{title}"},
        {"dialog": "true"},
        {"dialog": "true"}
    ],
    "search_episode": [
        "plugin://plugin.video.",
        {"keyboard": "{showname}"},
        {"dialog": "true"}
    ],
    "fallback": {
        "play_movie": "TG360_player_interno.json search_movie",
        "play_episode": "TG360_player_interno.json search_episode"
    }
}